function validate()
{
	
	console.log("validate called");
	var textflds =document.getElementsByTagNameById("input");
	console.log(textflds.length);
}

function init()
{
	var frmElement = document.getElementById("regForm");
	frmElement.addEventListener("submit",validate);
}
(function()
{
	init();
})();





























































































