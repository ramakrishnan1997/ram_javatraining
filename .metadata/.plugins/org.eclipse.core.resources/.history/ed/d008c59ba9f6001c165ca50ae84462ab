}

function withArgs(name)
{
	if(name==null)
	{
		document.write("Welcome Guest");
	}
	else
	{
        document.write("welcome"+name)
}	
}
var usdToInr =function(usd)
{
	return usd*0.80;
};
var usdToInr =function(inr)
{
	return usd=inr/0.80;
};

(function(){
	//simple();
	//withArgs('ram');
	//wthArgs();
	console.log("Multiply"+usdToInr(100));
	console.log("divide"+usdToInr(100));
})();