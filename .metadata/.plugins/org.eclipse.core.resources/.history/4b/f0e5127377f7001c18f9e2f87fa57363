function setcookie()
{
	var name="collor size";
	var value=40;
	document.cookie =name+"="+value;
}
function readcookie()
{
	var cookie = document.cookie;
	for(indpos in cookie)
	{
		cookie = cookie[indpos]
		if(cookie.name == "collor size")
		{
			
		}
	}
}
(function(){
	setcookie();
})();