<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>TestPracticeServlet</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>ContextServlet</servlet-name>
  <servlet-class>com.test.practice.ContextServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
   <servlet-name>ContextServlet</servlet-name>
   <url-pattern>/Arunservlet</url-pattern>
  </servlet-mapping>
  
  <context-param>
  <param-name>Comapany</param-name>
  <param-value>employee</param-value>
  </context-param>
  
  
  <servlet>
  <servlet-name>ConfigureServlet</servlet-name>
  <servlet-class>com.test.practice.ConfigureServlet</servlet-class>
  <init-param>
  <param-name>companyname</param-name>
  <param-value>Slindocompany</param-value>
  </init-param>
  </servlet>
  
  <servlet-mapping>
   <servlet-name>ConfigureServlet</servlet-name>
   <url-pattern>/Arunconfigure</url-pattern>
  </servlet-mapping>
  
  
  
  
  
</web-app>